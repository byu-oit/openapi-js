{
  "name": "@byu-oit/openapi.reference",
  "version": "0.0.0",
  "description": "OpenAPI reference object class and schema definitions",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "scripts": {
    "build": "concurrently 'npm run build:cjs' 'npm run build:esm' 'npm run build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf ./cjs ./esm ./types *.tsbuildinfo coverage",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@byu-oit/openapi.common": "*",
    "@sinclair/typebox": "^0.24.41",
    "lodash.get": "^4.4.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^28.1.8",
    "@types/lodash.get": "^4.4.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.0.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/byu-oit-sdk-js.git"
  },
  "keywords": [],
  "author": "Spencer Tuft <stuft2@byu.edu>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/byu-oit-sdk-js/issues"
  },
  "homepage": "https://github.com/byu-oit/byu-oit-sdk-js#readme",
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Reference Object"
  }
}

  "bugs": {
    "url": "https://github.com/byu-oit/byu-oit-sdk-js/issues"
  },
  "homepage": "https://github.com/byu-oit/byu-oit-sdk-js#readme",
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^5.4.3",
    "nx": "^14.5.10",
    "typedoc": "^0.23.14"
  }

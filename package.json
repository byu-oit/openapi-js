{
  "name": "@byu-oit/openapi-js",
  "version": "0.0.0",
  "description": "BYU OIT OpenAPI tools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/byu-oit-sdk-js.git"
  },
  "scripts": {
